set nocompatible
set backspace=indent,eol,start
syntax enable
set tabstop=4
set softtabstop=4
set number
set showcmd
set cursorline
filetype indent on
"set <PageUp>=<Ctrl-Y>
"set <PageDown>=<Ctrl-V>
nnoremap <C-Y> <PageUp>
nnoremap <C-J> <PageDown>
set term=ansi
colo desert
set tabstop=4 softtabstop=4 shiftwidth=4 noexpandtab
set ruler
set whichwrap+=<,>,h,l
